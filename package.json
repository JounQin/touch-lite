    "eslint-plugin-prettier": "^2.1.2",
    "prettier": "^1.5.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.0.0"
  }

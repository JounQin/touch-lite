{
  "name": "touch-lite",
  "version": "0.0.6",
  "description": "A lightweight touch library without any dependency.",
  "main": "dist/touch.min.js",
  "module": "lib/index.js",
  "unpkg": "dist/touch.js",
  "repository": "git@github.com:JounQin/touch-lite.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "base": "rollup -c --sourcemap",
